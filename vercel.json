{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "environment": {
            "NODE_ENV": "production"
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "headers": { "Cache-Control": "public, max-age=60" }
      }
    ],
    "redirects": [
      {
        "src": "/",
        "dest": "/magic/1",
        "status": 302
      }
    ]
  }