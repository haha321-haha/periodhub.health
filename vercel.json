{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "rewrites": [
      {
        "source": "/magic/:id*",
        "destination": "/magic/:id"
      }
    ],
    "headers": [
      {
        "source": "/_next/static/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      },
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=86400" }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/magic/1",
        "statusCode": 302
      }
    ],
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "framework": "nextjs"
  }